{
  "expo": {
    "name": "HeadX",
    "slug": "HeadX",
    "version": "1.0.4",
    "orientation": "portrait",
    "icon": "./assets/HeadX-adaptive-icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/HeadX-adaptive-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.HeadX.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSBluetoothAlwaysUsageDescription": "The app needs Bluetooth access to connect devices.",
        "NSBluetoothPeripheralUsageDescription": "The application uses Bluetooth to communicate with peripheral devices."
      },
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/HeadX-adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.HeadX.app",
      "runtimeVersion": "1.0.0"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-sqlite"
    ],
    "extra": {
      "eas": {
        "projectId": "bd287822-419f-4aa5-b740-21595c3ef34e"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/bd287822-419f-4aa5-b740-21595c3ef34e"
    }
  }
}
